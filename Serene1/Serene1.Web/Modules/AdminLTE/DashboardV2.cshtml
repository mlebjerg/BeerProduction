                $("#changingBtns").empty();
                $("#changingBtns").append(clearBtn);
                $('#changingBtns').prop('disabled', true);
                break;
            case 6:
                $("#stateShow").text("Execute");
                $("#changingBtns").empty();
                $("#changingBtns").append(resetBtn);
                $('#changingBtns').prop('disabled', true);
                break;
            case 7:
                $("#stateShow").text("Stopping");
                $("#changingBtns").empty();
                $("#changingBtns").append(resetBtn);
                $('#changingBtns').prop('disabled', true);
                break;
            case 8:
                $("#stateShow").text("Aborting");
                $("#changingBtns").empty();
                $("#changingBtns").append(clearBtn);
                $('#changingBtns').prop('disabled', true);
                break;
            case 9:
                $("#stateShow").text("Aborted");
                $("#changingBtns").empty();
