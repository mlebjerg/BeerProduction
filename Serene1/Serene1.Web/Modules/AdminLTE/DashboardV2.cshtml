                $('#changingBtns').prop('disabled', true);
                break;
            case 15:
                $("#stateShow").text("Resetting");
                $("#changingBtns").empty();
                $("#changingBtns").append(startBtn);
                $('#changingBtns').prop('disabled', true);
                break;
            case 16:
